# Environment
ENVIRONMENT=local
DEBUG=true

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=123456789012
AWS_S3_BUCKET_DATA=airflow-data-dev
AWS_S3_BUCKET_LOGS=airflow-logs-dev
AWS_S3_PREFIX=airflow/
AWS_SECRETS_BACKEND_ENABLED=true
AWS_SECRETS_PREFIX=airflow/
AWS_BEDROCK_MODEL_ID=anthropic.claude-3-5-sonnet-20241022-v2:0
AWS_BEDROCK_REGION=us-east-1

# Airflow Configuration
AIRFLOW_HOME=/opt/airflow
AIRFLOW__CORE__DAGS_FOLDER=/opt/airflow/dags
AIRFLOW__CORE__PLUGINS_FOLDER=/opt/airflow/plugins
AIRFLOW__CORE__EXECUTOR=LocalExecutor
AIRFLOW__CORE__LOAD_EXAMPLES=False
AIRFLOW__CORE__LOAD_DEFAULT_CONNECTIONS=False

# Database
AIRFLOW__DATABASE__SQL_ALCHEMY_CONN=postgresql+psycopg2://airflow:airflow@localhost:5432/airflow

# Secrets Backend
AIRFLOW__SECRETS__BACKEND=airflow.providers.amazon.aws.secrets.secrets_manager.SecretsManagerBackend
AIRFLOW__SECRETS__BACKEND_KWARGS={"connections_prefix":"airflow/connections","variables_prefix":"airflow/variables","region_name":"us-east-1"}

# Remote Logging
AIRFLOW__LOGGING__REMOTE_LOGGING=True
AIRFLOW__LOGGING__REMOTE_BASE_LOG_FOLDER=s3://airflow-logs-dev/logs
AIRFLOW__LOGGING__REMOTE_LOG_CONN_ID=aws_default

# Webserver
AIRFLOW__WEBSERVER__SECRET_KEY=your-secret-key-here-change-in-production
AIRFLOW__WEBSERVER__RBAC=True
